{
  "name": "ember-tutorial-component",
  "version": "0.0.4",
  "description": "Help tutorial using tooltips in UI, for example when any new feature is realsed and you want to show user how to use it.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "checkStyle": "jscs . --config=./.jscsrc --verbose",
    "fixStyle": "jscs . --config=./.jscsrc --fix --verbose"
  },
  "repository": "https://github.com/svkangal/ember-tutorial-component",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Shrinivas Kangal",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-ajax": "^2.0.1",
    "ember-cli": "2.7.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-less": "1.5.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-jshint": "^1.0.0",
    "ember-cli-qunit": "^2.0.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
