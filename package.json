  "keywords": [
    "ember-addon",
    "tutorial-component",
    "help-tooltips",
    "new-feature-tooltips",
    "ember-cli"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.6",
    "ember-cli-htmlbars": "^1.0.8",
    "ember-wormhole": "^0.4.0",
    "ember-cli-less": "1.5.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://svkangal.github.io/ember-tutorial-component/"
